{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                }
            }
        ],
        [
            "@babel/preset-react"
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-syntax-dynamic-import",
            {}
        ],
        [
            "@babel/plugin-proposal-object-rest-spread",
            {},
            "one"
        ],
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}